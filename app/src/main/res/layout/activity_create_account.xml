<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CreateAccount">

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="261dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="111dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="Last name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="70dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Contact number"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="133dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="201dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="150dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint="First name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="70dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="288dp"
        android:layout_height="50dp"
        android:text="@string/sign_up_button"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="340dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="123dp"
        android:layout_height="29dp"
        android:text="@string/sign_up_with"
        android:textAlignment="center"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="144dp"
        tools:layout_editor_absoluteY="415dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:text="@string/google_signin_button"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="471dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/facebook_signin_button"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="537dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:text="@string/terms_and_conditions"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="617dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:text="@string/data_collection"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="665dp" />

</androidx.constraintlayout.widget.ConstraintLayout>